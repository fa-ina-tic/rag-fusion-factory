# RAG Fusion Factory Environment Variables Template
# Copy this file to .env and modify the values as needed
# These environment variables override YAML configuration values

# Environment Selection
ENVIRONMENT=development

# API Configuration Overrides
RAG_API_HOST=0.0.0.0
RAG_API_PORT=8000
RAG_API_DEBUG=false

# Search Engine Configuration Overrides
RAG_SEARCH_TIMEOUT=30.0
RAG_MAX_CONCURRENT_ENGINES=10

# Model Configuration Overrides
RAG_MODEL_CACHE_DIR=./models

# Logging Configuration Overrides
RAG_LOG_LEVEL=INFO

# Training Configuration Overrides
RAG_TRAINING_BATCH_SIZE=32

# Normalization Configuration Overrides
RAG_NORMALIZATION_METHOD=min_max